{
  "name": "randyuebel.com",
  "version": "0.1.0",
  "homepage": "https://ruebel.github.io/personal-webiste",
  "main": "docs/index.js",
  "author": "Randy Uebel <randy.uebel@gmail.com>",
  "repository": "https://github.com/ruebel/personal-webiste",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "mv ./build ./docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "gsandf-react",
    "parser": "babel-eslint"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.1.7",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.7.1",
    "eslint-config-gsandf-react": "^0.2.0",
    "prettier": "^1.7.0"
  }
}
